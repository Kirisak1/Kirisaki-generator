{
  "author":  "kirisaki",
  "name": "kirisaki-generate",
  "basePackage": "com.github.kirisaki",
  "version": "1.0",
  "description": "代码生成器",
  "createTime": "2023-12-9",
  "fileConfig": {
    "inputPath":"",
    "outputPath": "generated",
    "type": "dir",
    "files": [
      {
        "inputPath": "src/com/kirisaki/acm/MainTemplate.java.ftl",
        "outputPath": "src/com/kirisaki/acm/MainTemplate.java",
        "type":  "file",
        "generateType": "dynamic"
      },
      {
        "inputPath": ".ignore",
        "outputPath": ".ignore",
        "type": "file",
        "generateType": "static"
      },
      {
        "inputPath": "README.md",
        "outputPath": "README.md",
        "type": "file",
        "generateType": "static"
      }
    ]
  },
  "modelConfig": {
    "models": [
      {
        "fieldName":"flag",
        "type": "boolean",
        "comment": "是否生成循环",
        "defaultValue": "false",
        "abbr": "l"
      },
      {
        "fieldName":"author",
        "type": "String",
        "comment": "作者注释",
        "defaultValue": "Kirisaki",
        "abbr": "a"
      },
      {
        "fieldName":"outputText",
        "type": "String",
        "comment": "输出信息",
        "defaultValue": "sum=",
        "abbr": "o"
      }
    ]
  }
}